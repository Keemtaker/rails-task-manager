<ul>
<% @tasks.each do |task| %>
  <li>
    <%= link_to task_path(task) do %>
    <h1><%= task.name %></h1>
    <% end %>
  <%= link_to "Edit this task", edit_task_path(task) %>
  <%= link_to "Create new task", new_task_path(task) %>
  <%= link_to "Delete this task", task_path(task), method: 'delete' %>
  </li>
  <% end %>
</ul>

